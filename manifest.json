{

    "description": "Extension pour une page d'accueil utile et apaisante",
    "manifest_version": 3,
    "name": "Notre super projet",
    "version": "1.0",
    "homepage_url": "https://github.com/adatechschool/extension-navigateur-chloe_aroun_nicolas_extensionnavigateur",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["scripts/content.js"]
        }
    ],
    "permissions": [
        "storage"
    ],
    "chrome_url_overrides" : {
        "newtab" : "index.html"
    }
}